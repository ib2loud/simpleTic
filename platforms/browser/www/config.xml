<?xml version='1.0' encoding='utf-8'?>
<widget id="toe.tac.simpletic" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>simpleTic</name>
    <description>
        A very simple single or two player Tic-Tac-Toe game.
    </description>
    <author email="bremy23@gmail.com" href="bremy23@gmail.com">
        Simply Simple Media
    </author>
    <content src="index.html" />
    <gap:plugin name="phonegap-admob" source="npm" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
